  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build  && compress-cra",
    "build:staging": "env-cmd -f .env.staging react-scripts build  && compress-cra",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "prod": "serve -s build",
    "start:prod": "serve -s build",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint ./src --ext ts --ext tsx --ext js",
    "format": "prettier --write ./src",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "check-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=14.17.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
